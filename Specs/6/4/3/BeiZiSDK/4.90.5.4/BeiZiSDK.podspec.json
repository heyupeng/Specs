{
  "name": "BeiZiSDK",
  "version": "4.90.5.4",
  "summary": "BeiZiSDK 广告平台, 用于请求广告的iOS SDK.",
  "description": "支持开屏, banner, 原生, 激励, 插屏, 原生插屏, 自定义类型广告请求。",
  "homepage": "https://gitee.com/adscope/bei-zi-sdk-pod.git",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "lvjunxue": "lvjunxue@hubcloud.com.cn"
  },
  "source": {
    "git": "https://gitee.com/adscope/bei-zi-sdk-pod.git",
    "tag": "4.90.5.4"
  },
  "platforms": {
    "ios": "10.0"
  },
  "libraries": [
    "z",
    "c++",
    "sqlite3",
    "xml2",
    "bz2",
    "c++abi",
    "resolv.9",
    "iconv",
    "c"
  ],
  "frameworks": [
    "AdSupport",
    "AVFoundation",
    "Accelerate",
    "CoreLocation",
    "CoreMotion",
    "CoreGraphics",
    "CoreTelephony",
    "CoreImage",
    "Foundation",
    "MobileCoreServices",
    "SystemConfiguration",
    "Security",
    "StoreKit",
    "WebKit",
    "UIKit",
    "ImageIO",
    "SafariServices"
  ],
  "weak_frameworks": "AppTrackingTransparency",
  "requires_arc": true,
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "xcconfig": {
    "VALID_ARCHS": "armv7 x86_64 arm64",
    "OTHER_LDFLAGS": "-ObjC"
  },
  "subspecs": [
    {
      "name": "BeiZiSDK",
      "vendored_frameworks": [
        "BeiZiSDK.xcframework",
        "BeiZiFoundation.xcframework"
      ]
    }
  ]
}
