        "Base/dummy.m",
        "Base/Sources/ARCore/module.modulemap"
      ],
      "frameworks": [
        "CoreTelephony",
        "Foundation",
        "SystemConfiguration",
        "Security",
        "UIKit"
      ],
      "libraries": [
        "c++",
        "z"
