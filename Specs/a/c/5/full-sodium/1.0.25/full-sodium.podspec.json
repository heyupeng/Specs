{
  "name": "full-sodium",
  "version": "1.0.25",
  "summary": "full build libsodium",
  "description": "full build libsodium, support more features",
  "homepage": "https://github.com/AddIcechan/full-sodium",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "ADDICE": "addice.ch@gmail.com"
  },
  "platforms": {
    "ios": "12.0",
    "osx": "10.13",
    "watchos": "8.0",
    "tvos": "13.0"
  },
  "source": {
    "git": "https://github.com/AddIcechan/full-sodium.git",
    "tag": "1.0.25"
  },
  "source_files": "Sources/full-sodium.h",
  "pod_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS": "$(inherited) \"${PODS_XCFRAMEWORKS_BUILD_DIR}/Clibsodium\"",
    "OTHER_SWIFT_FLAGS": "$(inherited) -no-verify-emitted-module-interface"
  },
  "user_target_xcconfig": {
    "SWIFT_INCLUDE_PATHS": "$(inherited) \"${PODS_XCFRAMEWORKS_BUILD_DIR}/Clibsodium\"",
    "OTHER_SWIFT_FLAGS": "$(inherited) -no-verify-emitted-module-interface"
  },
  "vendored_frameworks": "Clibsodium.xcframework",
  "public_header_files": "Sources/full-sodium.h",
  "static_framework": false,
  "requires_arc": true
}
