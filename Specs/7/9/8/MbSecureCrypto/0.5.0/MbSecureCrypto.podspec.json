{
  "name": "MbSecureCrypto",
  "version": "0.5.0",
  "summary": "A secure cryptography library for iOS and macOS",
  "homepage": "https://github.com/mavbozo/MbSecureCrypto",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Maverick Bozo": "mavbozo@pm.me"
  },
  "platforms": {
    "ios": "15.6",
    "osx": "12.4"
  },
  "swift_versions": [
    "5.0"
  ],
  "source": {
    "git": "file:///Users/mavbozo/yolo/ios/MbSecureCrypto",
    "tag": "0.5.0"
  },
  "source_files": [
    "MbSecureCrypto/*.{h,m,swift}",
    "MbSecureCrypto/**/*.{h,m,swift}"
  ],
  "public_header_files": [
    "MbSecureCrypto/*.h",
    "MbSecureCrypto/Random/*.h",
    "MbSecureCrypto/Cipher/*.h"
  ],
  "module_name": "MbSecureCrypto",
  "frameworks": [
    "Security",
    "Foundation"
  ],
  "pod_target_xcconfig": {
    "MARKETING_VERSION": "0.5.0",
    "CURRENT_PROJECT_VERSION": "1",
    "IPHONEOS_DEPLOYMENT_TARGET": "15.6",
    "MACOSX_DEPLOYMENT_TARGET": "12.4",
    "SWIFT_VERSION": "5.0",
    "DEFINES_MODULE": "YES",
    "SWIFT_INCLUDE_PATHS": "$(PODS_TARGET_SRCROOT)/MbSecureCrypto",
    "SWIFT_OBJC_INTERFACE_HEADER_NAME": "MbSecureCrypto-Swift.h",
    "OTHER_CFLAGS": "-fno-common -fstack-protector-strong",
    "CLANG_ENABLE_MODULES": "YES",
    "CLANG_WARN_OBJC_IMPLICIT_RETAIN_SELF": "YES",
    "GCC_TREAT_WARNINGS_AS_ERRORS": "YES",
    "PRODUCT_NAME": "$(TARGET_NAME)",
    "PRODUCT_BUNDLE_IDENTIFIER": "com.mavbozo.mbsecurecrypto",
    "SWIFT_OPTIMIZATION_LEVEL[config=Debug]": "-Onone",
    "SWIFT_COMPILATION_MODE": "wholemodule"
  },
  "requires_arc": true,
  "swift_version": "5.0"
}
